{
  "name": "hcc-debugger",
  "version": "1.2.0",
  "private": false,
  "engines": {
    "node": ">=18.20.8",
    "npm": ">=8.19.4"
  },
  "scripts": {
    "build": "fec build",
    "deploy": "npm-run-all build lint test",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src",
    "lint:js:fix": "eslint src --fix",
    "patch:hosts": "fec patch-etc-hosts",
    "start": "HOT=true fec dev",
    "static": "fec static",
    "test": "jest",
    "postinstall": "ts-patch install && rimraf .cache",
    "verify": "npm-run-all build lint test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook"
  },
  "dependencies": {
    "@patternfly/react-core": "^6.4.0",
    "@patternfly/react-data-view": "^6.4.0",
    "@patternfly/react-table": "^6.4.0",
    "@redhat-cloud-services/frontend-components": "^7.0.13",
    "@redhat-cloud-services/frontend-components-notifications": "^6.1.14",
    "@redhat-cloud-services/frontend-components-utilities": "^7.0.9",
    "@scalprum/core": "^0.9.0",
    "@scalprum/react-core": "^0.11.1",
    "@types/js-yaml": "^4.0.9",
    "@unleash/proxy-client-react": "^3.6.0",
    "js-yaml": "^4.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2"
  },
  "devDependencies": {
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^3.0.1",
